{
  "name": "@lingo.dev/compiler",
  "version": "0.0.1",
  "description": "Lingo.dev Compiler Beta",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "build/index.cjs",
  "types": "build/index.d.cts",
  "module": "build/index.mjs",
  "exports": {
    ".": {
      "types": "./build/plugin/index.d.cts",
      "import": "./build/plugin/index.mjs",
      "require": "./build/plugin/index.cjs"
    },
    "./react": {
      "react-server": {
        "types": "./build/react/server/index.d.ts",
        "import": "./build/react/server/index.mjs",
        "default": "./build/react/server/index.cjs"
      },
      "default": {
        "types": "./build/react/client/index.d.ts",
        "import": "./build/react/client/index.mjs",
        "default": "./build/react/client/index.cjs"
      }
    },
    "./virtual/config": {
      "types": "./build/virtual/config.d.ts",
      "import": "./build/virtual/config.mjs",
      "require": "./build/virtual/config.cjs"
    },
    "./virtual/locale/server": {
      "types": "./build/virtual/locale/server.d.ts",
      "import": "./build/virtual/locale/server.mjs",
      "require": "./build/virtual/locale/server.cjs"
    },
    "./virtual/locale/client": {
      "types": "./build/virtual/locale/client.d.ts",
      "import": "./build/virtual/locale/client.mjs",
      "require": "./build/virtual/locale/client.cjs"
    },
    "./react/client": {
      "types": "./build/react/index.d.ts",
      "import": "./build/react/index.mjs",
      "require": "./build/react/index.cjs"
    },
    "./react/next": {
      "react-server": {
        "types": "./build/react/next/server.d.ts",
        "import": "./build/react/next/server.mjs",
        "default": "./build/react/next/server.cjs"
      },
      "default": {
        "types": "./build/react/next/client.d.ts",
        "import": "./build/react/next/client.mjs",
        "default": "./build/react/next/client.cjs"
      }
    },
    "./react/config": {
      "types": "./build/react/config.d.ts",
      "import": "./build/react/config.mjs",
      "require": "./build/react/config.cjs"
    },
    "./react/server": {
      "types": "./build/react/server-only/index.d.ts",
      "import": "./build/react/server-only/index.mjs",
      "require": "./build/react/server-only/index.cjs"
    },
    "./next": {
      "types": "./build/plugin/next.d.ts",
      "import": "./build/plugin/next.mjs",
      "require": "./build/plugin/next.cjs"
    },
    "./vite": {
      "types": "./build/plugin/vite.d.ts",
      "import": "./build/plugin/vite.mjs",
      "require": "./build/plugin/vite.cjs"
    },
    "./webpack": {
      "types": "./build/plugin/webpack.d.ts",
      "import": "./build/plugin/webpack.mjs",
      "require": "./build/plugin/webpack.cjs"
    },
    "./next-compiler-loader": {
      "types": "./build/plugin/next-compiler-loader.d.ts",
      "import": "./build/plugin/next-compiler-loader.mjs",
      "require": "./build/plugin/next-compiler-loader.cjs"
    },
    "./next-config-loader": {
      "types": "./build/plugin/next-config-loader.d.ts",
      "import": "./build/plugin/next-config-loader.mjs",
      "require": "./build/plugin/next-config-loader.cjs"
    },
    "./next-locale-server-loader": {
      "types": "./build/plugin/next-locale-server-loader.d.ts",
      "import": "./build/plugin/next-locale-server-loader.mjs",
      "require": "./build/plugin/next-locale-server-loader.cjs"
    },
    "./next-locale-client-loader": {
      "types": "./build/plugin/next-locale-client-loader.d.ts",
      "import": "./build/plugin/next-locale-client-loader.mjs",
      "require": "./build/plugin/next-locale-client-loader.cjs"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "pnpm typecheck && tsdown",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf build",
    "test": "vitest --run",
    "test:watch": "vitest -w",
    "test:prepare": "tsx tests/helpers/prepare-fixtures.ts",
    "test:e2e": "playwright test",
    "test:e2e:next": "playwright test --grep next",
    "test:e2e:vite": "playwright test --grep vite",
    "test:e2e:shared": "playwright test tests/e2e/shared",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "playwright:install": "playwright install chromium"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^22.10.5",
    "@types/proper-lockfile": "^4.1.4",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "next": "^16.0.3",
    "tsdown": "^0.16.5",
    "tsx": "^4.19.2",
    "typescript": "^5.9.3",
    "unplugin": "^2.3.10",
    "vitest": "^4.0.13"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.19",
    "@ai-sdk/groq": "^1.2.3",
    "@ai-sdk/mistral": "^1.2.8",
    "@babel/core": "^7.26.0",
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@formatjs/icu-messageformat-parser": "^2.11.4",
    "@openrouter/ai-sdk-provider": "^0.7.1",
    "ai": "^4.3.19",
    "dotenv": "^16.4.5",
    "fast-xml-parser": "^5.0.8",
    "intl-messageformat": "^10.7.18",
    "lingo.dev": "^0.117.0",
    "lodash": "^4.17.21",
    "ollama-ai-provider": "^1.2.0",
    "proper-lockfile": "^4.1.2",
    "ws": "^8.18.3"
  },
  "peerDependencies": {
    "next": "^15.0.0 || ^16.0.4",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
